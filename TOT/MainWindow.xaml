<Window x:Class="TOT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TOT"
        xmlns:userControls="clr-namespace:TOT.View.UserControls"
        mc:Ignorable="d"
        Title="TOT" Height="450" Width="800">
    <Grid x:Name="MainGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="24"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <userControls:MenuBar x:Name="menuBar"/>
        <userControls:WelcomeUI x:Name="welcomeUI" Grid.Row="1"/>
        <userControls:SingleImageUI x:Name="singleImageUI" Grid.Row="1" Visibility="Hidden"/>
    </Grid>
    
</Window>
